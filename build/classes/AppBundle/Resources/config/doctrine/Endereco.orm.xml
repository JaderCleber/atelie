<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Endereco" table="endereco">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idcidade" type="integer" column="idCidade" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tendereco" type="string" column="tEndereco" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tcep" type="string" column="tCep" length="9" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tnumero" type="string" column="tNumero" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tcomplemento" type="string" column="tComplemento" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tobservacao" type="string" column="tObservacao" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
