<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Usuario" table="usuario">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tlogin" type="string" column="tLogin" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tsenha" type="string" column="tSenha" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tnome" type="string" column="tNome" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idnivel" type="integer" column="idNivel" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
