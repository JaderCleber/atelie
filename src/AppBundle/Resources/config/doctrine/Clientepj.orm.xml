<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Clientepj" table="clientePj">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tnomefantasia" type="string" column="tNomeFantasia" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tcpfresponsavel" type="string" column="tCpfResponsavel" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dcadastro" type="datetime" column="dCadastro" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="trazaosocial" type="string" column="tRazaoSocial" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tcnpj" type="string" column="tCnpj" length="18" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
